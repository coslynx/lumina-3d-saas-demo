# Environment variables for Lumina SaaS 3D Landing Page

# Application title
VITE_APP_TITLE="Lumina SaaS 3D Demo"

# Public URL where the app is served
VITE_PUBLIC_URL="/"

# Backend API base URL
VITE_API_BASE_URL="https://api.example.com"

# Enable High Contrast Mode (true or false)
VITE_HIGH_CONTRAST_MODE=false

# Google Analytics tracking ID (optional)
VITE_GA_TRACKING_ID=""

# Optimize 3D performance (true or false)
VITE_OPTIMIZE_3D_PERFORMANCE=true

# Reduce ThreeJs complexity for Iower end (true or false)
VITE_REDUCE_COMPLEXITY=false

# Enable Google Analytics Tracking
VITE_ENABLE_ANALYTICS=false

# Toggle ray tracing features
VITE_ENABLE_3D_INTERACTIONS=true

# Public path for the 3d models
VITE_THREE_MODEL_PATH="/models/"

# Build target environment (development or production)
VITE_BUILD_TARGET="development"

# WARNING: Do not include actual API keys or other sensitive information in this file.
# Use environment variables or a secrets management system to securely manage these values.